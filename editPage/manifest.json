{
  "name": "Edit Page",
  "description": "Change the current page editable",
  "version": "1.0",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "background": { "scripts": ["editable.js"] },
  "browser_action": {
      "default_title": "Set this page editable.",
      "default_icon": "icon.png"
  },
  "manifest_version": 2
}
